# Railway Environment Variables
NODE_ENV=production
PORT=3000

# Database (Railway PostgreSQL)
DB_HOST=${PGHOST}
DB_PORT=${PGPORT}
DB_NAME=${PGDATABASE}
DB_USER=${PGUSER}
DB_PASSWORD=${PGPASSWORD}

# JWT
JWT_SECRET=${JWT_SECRET}
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=${JWT_REFRESH_SECRET}

# CORS
CORS_ORIGIN=${CORS_ORIGIN}

# Logging
LOG_LEVEL=info
